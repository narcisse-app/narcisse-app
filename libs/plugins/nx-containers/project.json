{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/plugins/nx-containers/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/plugins/nx-containers",
        "main": "libs/plugins/nx-containers/src/index.ts",
        "tsConfig": "libs/plugins/nx-containers/tsconfig.lib.json",
        "assets": [
          "libs/plugins/nx-containers/*.md",
          {
            "input": "./libs/plugins/nx-containers/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/plugins/nx-containers/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/plugins/nx-containers",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/plugins/nx-containers",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/plugins/nx-containers/**/*.ts",
          "libs/plugins/nx-containers/generators.json",
          "libs/plugins/nx-containers/executors.json",
          "libs/plugins/nx-containers/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/plugins/nx-containers"],
      "options": {
        "jestConfig": "libs/plugins/nx-containers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
